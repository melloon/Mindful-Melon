{
    "name": "Mindful Melon",
    "description": "An extension made by Team Goofy Goober for Geaux Hack 2025",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "timer.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "activeTab",
        "storage",
        "notifications",
        "alarms"
    ]


}